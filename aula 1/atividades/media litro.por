programa
{
	
	funcao inicio()
	{
		real km_rodados, gas_gasto, media

		escreva("Digite os Km rodados: ", "\n")
		leia(km_rodados)

		escreva("Digite a gasolina gasta: ", "\n")
		leia(gas_gasto)

		media = km_rodados / gas_gasto

		escreva("A media foi de ", media)

		
	}
}
